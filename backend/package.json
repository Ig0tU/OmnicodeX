{
  "name": "autonomous-agent-backend",
  "version": "1.0.0",
  "description": "Backend server for Autonomous Agent Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:setup": "node -r ts-node/register src/db/setup.ts",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests specified yet\" && exit 0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "puppeteer": "^21.5.2",
    "@google/generative-ai": "^0.2.1",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "@types/node": "^20.8.7",
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "@types/pg": "^8.10.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  },
  "keywords": ["autonomous", "agent", "ai", "automation", "puppeteer", "gemini"],
  "author": "CloudIDE",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
